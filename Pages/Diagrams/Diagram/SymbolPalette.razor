    public void ModeChange(Syncfusion.EJ2.Blazor.DropDowns.ChangeEventArgs<string> args)
    {
        if (args.Value == "Multiple")
        {
            this.expandMode = Syncfusion.EJ2.Blazor.Navigations.ExpandMode.Multiple;
        }
        else
        {
            this.expandMode = Syncfusion.EJ2.Blazor.Navigations.ExpandMode.Single;

        }
        this.StateHasChanged();
    }

    private void sizeChange(Syncfusion.EJ2.Blazor.Inputs.ChangeEventArgs args)
    {
        this.symbolWidth = args.Value;
        this.symbolHeight = args.Value;
        this.StateHasChanged();
    }


    private void animationChange(bool isChecked)
    {
        this.animationChecked = isChecked;
        this.animationMode = isChecked;
        this.StateHasChanged();
    }

    private void textChange(bool isChecked)
    {

    }


    private void iconChange(bool isChecked)
    {
        if (isChecked)
