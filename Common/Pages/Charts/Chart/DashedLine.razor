    protected override void OnInitialized()
    {
        Theme = ThemeHelper.GetCurrentTheme(NavigationManager.Uri);
        if (SampleService != null && SampleService.IsDevice)
        {
            Rotation = -45;
            Label = LabelIntersectAction.None;
            Width = "100%";
        }
    }

    private void AxisLabelRender(AxisLabelRenderEventArgs args)
    {
        if (args.Axis.Interval == 2e9)
        {
            args.Text = (Convert.ToDouble(args.Text) / 1e9).ToString("0");
        }
    }

    public void SharedTooltipRender(SharedTooltipRenderEventArgs args)
    {
        if (args.Data is List<PointInfo> dataList && dataList.Count > 0)
        {
            List<string> tooltipText = new List<string>();

            foreach (var pointInfo in dataList)
            {
                double pointYValue = Convert.ToDouble(pointInfo.PointY);
                string value = pointYValue.ToString("N0", System.Globalization.CultureInfo.InvariantCulture);

                if (pointYValue >= 1_000_000_000)
                {
                    value = (pointYValue / 1_000_000_000).ToString("0.##") + " B";
                }
                else if (pointYValue >= 1_000_000)
                {
                    value = (pointYValue / 1_000_000).ToString("0.##") + " M";
                }
                if (!string.IsNullOrEmpty(pointInfo.SeriesName))
                {
                    tooltipText.Add($"{pointInfo.SeriesName}: <b>{value}</b>");
                }
                else 
                {
                    int index = dataList.IndexOf(pointInfo);
                    string defaultTooltip = args.Text[index]?.Split(':')[0] ?? string.Empty;
                    tooltipText.Add($"{defaultTooltip}: <b>{value}</b>");
                }
            }
            args.Text = tooltipText;
        }
    }

