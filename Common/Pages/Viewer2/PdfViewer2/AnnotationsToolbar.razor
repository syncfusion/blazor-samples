@{ #endif }
@*End:Server*@
</ActionDescription>

<div class="control-section">

     <SfPdfViewer2 @ref="Viewer" DocumentPath="@DocumentPath" EnableAnnotationToolbar="true" CommentPanelVisible="true" Height="640px" Width="100%" DownloadFileName="Annotation">
