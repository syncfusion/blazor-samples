@page "/listview/call-history"

@using Syncfusion.Blazor.Lists;
@using Syncfusion.Blazor.Navigations;
@*Hidden:Lines*@
@inherits SampleBaseComponent
@*End:Hidden*@

<SampleDescription>
    <p>This sample demonstrates the call history application using ListView component. Click the checklist to filter the data in the contacts list. </p>
</SampleDescription>
<ActionDescription>
    <p>This sample filters out the data from ListView based on the data selected from the checklist. Here, ListView utilizes the <a target="_blank" class="code" href="https://help.syncfusion.com/cr/blazor/Syncfusion.Blazor~Syncfusion.Blazor.Lists.SfListView%601~Template.html" aria-label="Class reference of Template property in ListView">Template</a> and <a target="_blank" class="code" href="https://help.syncfusion.com/cr/blazor/Syncfusion.Blazor~Syncfusion.Blazor.Lists.SfListView%601~ShowIcon.html" aria-label="Class reference of ShowIcon property in ListView">ShowIcon</a> properties to represent the call history application. The Tab component is used in this sample for navigation purposes. </p>
</ActionDescription>

<div class="col-lg-12 control-section">
    <div class="layoutWrapper">
        <div class="speaker">
            <div class="camera"></div>
        </div>
        <div class="layout">
            <div id="list-container">
                <div class="tabContainer">
                    @*Initialization of Tab component*@
                    <SfTab CssClass="tab">
                        <TabItems>
                            <TabItem>
                                <ChildContent>
                                    <TabHeader Text="All"></TabHeader>
                                </ChildContent>
                                <ContentTemplate>
                                    @* Initialization of ListView with call history data *@
                                    <SfListView CssClass="e-list-template all-calls" DataSource="@ListViewDataSource.CallHistories">
                                        <ListViewFieldSettings TValue="CallHistoryModel" Id="Id" Text="Text" GroupBy="Category"></ListViewFieldSettings>
                                        <ListViewTemplates TValue="CallHistoryModel">
                                            <Template>
                                                @{
                                                    <div class="e-list-wrapper e-list-avatar e-list-multi-line">
                                                        <span class="e-avatar e-avatar-circle e-icon @context.Text.ToLower()"></span>
                                                        <span class="e-list-item-header">@context.Text</span>
                                                        <span class="@context.Type e-list-content">
                                                            @context.Group, @context.Time
                                                        </span>
                                                    </div>
                                                }
                                            </Template>
                                        </ListViewTemplates>
                                    </SfListView>
                                </ContentTemplate>
                            </TabItem>
                            <TabItem>
                                <ChildContent>
                                    <TabHeader Text="Received"></TabHeader>
                                </ChildContent>
                                <ContentTemplate>
                                    @* Initialization of ListView with received calls data *@
                                    <SfListView CssClass="e-list-template received" DataSource="@ListViewDataSource.GetReceivedData()">
                                        <ListViewFieldSettings TValue="CallHistoryModel" Id="Id" Text="Text" GroupBy="Category"></ListViewFieldSettings>
                                        <ListViewTemplates TValue="CallHistoryModel">
                                            <Template>
                                                @{
                                                    <div class="e-list-wrapper e-list-avatar e-list-multi-line">
                                                        <span class="e-avatar e-avatar-circle e-icon @context.Text.ToLower()"></span>
                                                        <span class="e-list-item-header">@context.Text</span>
                                                        <span class="@context.Type e-list-content">
                                                            @context.Group, @context.Time
                                                        </span>
                                                    </div>
                                                }
                                            </Template>
                                        </ListViewTemplates>
                                    </SfListView>
                                </ContentTemplate>
                            </TabItem>
                            <TabItem>
                                <ChildContent>
                                    <TabHeader Text="Missed"></TabHeader>
                                </ChildContent>
                                <ContentTemplate>
                                    @* Initialization of ListView with missed calls data *@
                                    <SfListView CssClass="e-list-template missed" DataSource="@ListViewDataSource.GetMissedData()">
                                        <ListViewFieldSettings TValue="CallHistoryModel" Id="Id" Text="Text" GroupBy="Category"></ListViewFieldSettings>
                                        <ListViewTemplates TValue="CallHistoryModel">
                                            <Template>
                                                @{
                                                    <div class="e-list-wrapper e-list-avatar e-list-multi-line">
                                                        <span class="e-avatar e-avatar-circle e-icon @context.Text.ToLower()"></span>
                                                        <span class="e-list-item-header">@context.Text</span>
                                                        <span class="@context.Type e-list-content">
                                                            @context.Group, @context.Time
                                                        </span>
                                                    </div>
                                                }
                                            </Template>
                                        </ListViewTemplates>
                                    </SfListView>
                                </ContentTemplate>
                            </TabItem>
                        </TabItems>
                    </SfTab>
                </div>
            </div>
        </div>
        <div class="outerButton"> </div>
    </div>
</div>

