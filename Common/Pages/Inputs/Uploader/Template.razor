@page "/File-Upload/Template"

@using Syncfusion.Blazor.Inputs

@*Hidden:Lines*@
@inherits SampleBaseComponent
@inject NavigationManager NavigationManager


@*End:Hidden*@

<SampleDescription>
    <p>This File Upload example demonstrates how to customize the file list with the template. Browse or select the files to view the file list template.</p>
</SampleDescription>
<ActionDescription>
    <p>The File Upload allows to customize its file list using the template. The template is used for each file in the file list.</p>
</ActionDescription>

<div class="col-lg-12 control-section">
    <div class="control_wrapper">
        <div class="col-lg-6" id="dropArea">
            <SfUploader @ref="FileUploader" CssClass="@CssClass" AutoUpload="true" AllowedExtensions="@ExtensionAllowed">
                <UploaderButtons Browse="Browse"></UploaderButtons>
                <UploaderEvents ValueChange="OnChangeUpload" FileSelected="AfterSelect" Success="OnSuccess" OnFailure="OnFailure"></UploaderEvents>
                <UploaderTemplates>
                    <Template>
                        <span class='icon template-icons sf-icon-@(context.Type)'></span>
                        <span class="e-file-container">
                            <span class="e-file-name">@context.Name</span>
                            <span class="e-file-status">@context.Status</span>
                        </span>
                        <span class="e-icons e-file-delete-btn m-2 position-relative" tabindex="0" title="Delete @(context.Name)" @onclick="@(() => DeleteFile(context))" />
                    </Template>
                </UploaderTemplates>
            </SfUploader>
        </div>
    </div>
</div>

@code {
    public SfUploader? FileUploader;
    public string CssClass = "custom-class";
    public string ExtensionAllowed { get; set; } = ".pdf, .txt, .png";
    public List<Syncfusion.Blazor.Inputs.FileInfo> files = new List<Syncfusion.Blazor.Inputs.FileInfo>();

    public void AfterSelect(SelectedEventArgs args)
    {
        string[] Extension = { "pdf", "txt", "png" };
        if (Extension.ToList().IndexOf(args.FilesData[0].Type) < 0)
        {
            args.Cancel = true;
        }
    }
    private void DeleteFile(Syncfusion.Blazor.Inputs.FileInfo file)
    {
        var filename = file.Name;
        int idx = files.FindIndex(c => c.Name == filename);
        FileInfo[] text = new FileInfo[] { files[idx] };
        if (this.FileUploader != null)
        {
            this.FileUploader.RemoveAsync(text);           
        }
    }
    private void OnChangeUpload(UploadChangeEventArgs args)
    {
        files = new List<Syncfusion.Blazor.Inputs.FileInfo>();
        if (args.Files != null)
        {
            foreach (var file in args.Files)
            {
                files.Add(new Syncfusion.Blazor.Inputs.FileInfo()
                    {
                        Name = file.FileInfo.Name,
                        Id = file.FileInfo.Id,
                        RawFile = file.FileInfo.RawFile,
                        Size = file.FileInfo.Size,
                        Status = file.FileInfo.Status,
                        StatusCode = file.FileInfo.StatusCode,
                        FileSource = file.FileInfo.FileSource,
                        Input = file.FileInfo.Input,
                        List = file.FileInfo.List,
                        Type = file.FileInfo.Type,
                        ValidationMessages = file.FileInfo.ValidationMessages
                    });
            }
            
        }
    }
    public void OnSuccess(SuccessEventArgs args)
    {
        this.CssClass = "custom-class file-success";
    }
    public void OnFailure(FailureEventArgs args)
    {
        this.CssClass = "custom-class file-failed";
    }
}

<style>
    .control_wrapper {
        margin-left: 250px;
    }
    .custom-class.file-success .e-file-status {
        color: green !important;
    }
    .custom-class.file-success .e-file-failed {
        color: red !important;
    }
    .e-upload.e-control-wrapper.custom-class {
        border: 1px dashed #c3c3cc;
        height: 57px;
        width: 500px;
    }

    .e-upload .e-upload-files {
        border: 1px solid #e9ecef;
    }

    .custom-class button.e-css.e-btn.e-upload-browse-btn {
        margin-left: 130px;
        background: none !important;
        border: none;
        padding: 0 !important;
        font-family: inherit;
        color: #ff4081;
        text-decoration: underline;
        cursor: pointer;
        box-shadow: none;
        text-transform: capitalize;
    }

    .custom-class button.e-css.e-btn.e-upload-browse-btn:hover {
        background: none !important;
        border: none;
        padding: 0 !important;
        font-family: inherit;
        color: #ff4081;
        text-decoration: underline;
        cursor: pointer;
        box-shadow: none;
        text-transform: capitalize;
    }

    .custom-class.e-upload .e-file-drop {
        margin-left: 1px;
    }

    .custom-class .e-file-container {
        display: inline-block !important;
        margin: 9px 0 0 65px !important;
        width: 360px;
    }

    .custom-class .e-file-status {
        padding-top: 4px !important;
    }

    .custom-class .e-upload-progress-wrap {
        padding-top: 12px !important;
    }

    .custom-class .e-file-name {
        display: contents;
    }

    .custom-class .e-upload-file-list {
        min-height: 75px !important;
    }

    .custom-class .icon.template-icons {
        top: 8px !important;
        position: absolute;
        left: 10px;
    }

    [class^="sf-icon-"], [class*=" sf-icon-"] {
        font-family: 'FiletypeV2 Font' !important;
        speak: none;
        font-size: 40px;
        font-style: normal;
        font-weight: normal;
        font-variant: normal;
        text-transform: none;
        line-height: 1;
        -webkit-font-smoothing: antialiased;
        -moz-osx-font-smoothing: grayscale;
        position: relative;
    }

    .sf-icon-png:before, .sf-icon-PNG:before {
        content: "\e70a";
    }

    .sf-icon-pdf:before, .sf-icon-PDF:before {
        content: "\e70c";
    }

    .sf-icon-txt:before, .sf-icon-TXT:before {
        content: "\e70d";
    }

    @@font-face {
        font-family: 'FiletypeV2 Font';
        src: url(data:application/x-font-ttf;charset=utf-8;base64,) format('truetype');
        font-weight: normal;
        font-style: normal;
    }
</style>
