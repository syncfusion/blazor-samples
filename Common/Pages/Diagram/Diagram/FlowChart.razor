        };
        PointPort port2 = new PointPort()
        {
            ID = "port2",
            Shape = PortShapes.Circle,
            Offset = new DiagramPoint() { X = 0.5, Y = 0 }
        };
        PointPort port3 = new PointPort()
        {
            ID = "port3",
            Shape = PortShapes.Circle,
            Offset = new DiagramPoint() { X = 1, Y = 0.5 }
        };
        PointPort port4 = new PointPort()
