            }
        }
    }

    public void OnDocumentChange()
    {
        string name = container.DocumentEditor.DocumentName;
        if (name != "")
        {
            DocumentName = name;
        }
    }

    public async void OnToolbarClick(Syncfusion.Blazor.DocumentEditor.ClickEventArgs args)
    {
        switch (args.Item.Id)
        {
            case "mergeDocument":
                this.SpinnerVisible = true;
                await Merge();
                this.SpinnerVisible = false;
                break;
            case "insertField":
                await ShowInsertFielddialog();
                break;
        }
    }

    public async Task Merge()
    {
        
        SfDocumentEditor editor = container.DocumentEditor;
        string base64Data = await editor.SaveAsBlobAsync(FormatType.Docx);
        byte[] data = Convert.FromBase64String(base64Data);
        //To observe the memory go down, null out the reference of base64Data variable.
        base64Data = null;
        //Word document file stream
        Stream stream = new MemoryStream();
        stream.Write(data, 0, data.Length);
        stream.Position = 0;
        //To observe the memory go down, null out the reference of data variable.
        data = null;
        Syncfusion.DocIO.DLS.WordDocument doc = new Syncfusion.DocIO.DLS.WordDocument(stream, Syncfusion.DocIO.FormatType.Docx);
        doc.MailMerge.RemoveEmptyGroup = true;
        doc.MailMerge.RemoveEmptyParagraphs = true;
        doc.MailMerge.ClearFields = true;
        doc.MailMerge.Execute(CustomerDataModel.GetAllRecords());
        doc.Save(stream, Syncfusion.DocIO.FormatType.Docx);
        string sfdtText = "";
        WordDocument document1 = WordDocument.Load(stream, ImportFormatType.Docx);
        stream.Dispose();
        sfdtText = JsonSerializer.Serialize(document1);
        await container.DocumentEditor.OpenAsync(sfdtText);
        document1.Dispose();
        doc.Dispose();
    }


    private async Task ShowInsertFielddialog()
    {
        string fieldName = await DialogService.PromptAsync("Field Name:", "Merge Field");
        if (fieldName != null)
        {
            await container.DocumentEditor.Editor.InsertFieldAsync("MERGEFIELD " + fieldName + " \\* MERGEFORMAT");
        }
    }

