@page "/document-editor/custom-context-menu"

@using Syncfusion.Blazor.DocumentEditor
@using Syncfusion.Blazor.Buttons
@inject Microsoft.AspNetCore.Components.NavigationManager UriHelper
@inject IJSRuntime JSRuntime;
@using BlazorSamples.DocumentData;
@*Hidden:Lines*@
@inherits SampleBaseComponent
@inject NavigationManager NavigationManager


@*End:Hidden*@

<SampleDescription>
    <p>This example demonstrates how to add custom option in context menu and some customization for context menu.</p>
</SampleDescription>
<ActionDescription>
   <p>In this example, custom options are added to search the selected text in google. The custom options will be visible after selecting the text.</p>
   <p style='display: block'>Context Menu Customization Features</p>
     <ul>
        <li>Hide existing menu item and show custom menu item alone. It will be achieved by passing boolean value in second parameter of AddCustomMenu() method</li>
        <li>Show the custom menu item in bottom of the exisiting item. It will be achieved by passing boolean value in third parameter of AddCustomMenu() method</li>
         <li>Custom Option Functionality achieved by using the <code>ContextMenuItemSelected</code> Event</li>
      </ul>
    <p style=""display: block""> More information about the document editor features can be found in this <a target=""_blank"" href="https://blazor.syncfusion.com/documentation/document-editor/" aria-label="Blazor DocumentEditor Overview documentation">documentation section. </a></p>
</ActionDescription>

<div class="control-section">
    <div>
        <div id='documenteditor_titlebar' class="e-de-ctn-title">
            <div class="single-line" id="documenteditor_title_contentEditor" style="border: 1px solid transparent;" title="Document Name." contenteditable="false">
                <label id="documenteditor_title_name" class="doc-edit-title"> @DocumentName</label>
            </div>
            <div style="float: right">
                <SfButton IconCss="e-de-icon-Print" CssClass="title-bar-btn" @onclick="Print">Print</SfButton>
            </div>
            <div style="float: right">
                <SfButton IconCss="e-de-icon-Download" CssClass="title-bar-btn" @onclick="OnExport">Download</SfButton>
            </div>
        </div>
        <div>
            <SfDocumentEditorContainer @ref="container" Height="590px" RestrictEditing="@ReadOnly" ShowPropertiesPane="@ShowProperties" DocumentEditorSettings="settings">
                <DocumentEditorContainerEvents Created="OnCreated" DocumentChanged="OnDocumentChange" ContextMenuItemSelected="OnContentMenuSelect"></DocumentEditorContainerEvents>
            </SfDocumentEditorContainer>
        </div>
    </div>
</div>


@code {

    internal string DocumentName { get; set; }
    internal bool ReadOnly { get; set;  }
    internal bool ShowProperties { get; set; } = true;
    SfDocumentEditorContainer container;
    DocumentEditorSettingsModel settings = new DocumentEditorSettingsModel() { ShowRuler = true};

    public void OnExport(object args)
    {
        SfDocumentEditor documentEditor = container.DocumentEditor;
        documentEditor.SaveAsync(DocumentName, FormatType.Docx);
    }

    public void Print(object args)
    {
        SfDocumentEditor documentEditor = container.DocumentEditor;
        documentEditor.PrintAsync();
    }

    public async void OnCreated(object args)
    {
        SfDocumentEditor documentEditor = container.DocumentEditor;
        if (documentEditor != null)
        {
            await documentEditor.OpenAsync(Data.DefaultDocument);
            List<Syncfusion.Blazor.DocumentEditor.MenuItemModel> contentMenuItem = new List<Syncfusion.Blazor.DocumentEditor.MenuItemModel>
            {
            new Syncfusion.Blazor.DocumentEditor.MenuItemModel { Text="Search In Google", Id= "search_in_google", IconCss="e-icons e-de-ctnr-find" }
            };
            documentEditor.ContextMenu.AddCustomMenu(contentMenuItem, false, false);
            DocumentName = "Custom Context Menu";
            if (SampleService.IsDevice)
            {
                ReadOnly = true;
                ShowProperties = false;
                await documentEditor.FitPageAsync(PageFitType.FitPageWidth);
            }
        }
    }

    public void OnDocumentChange()
    {
        string name = container.DocumentEditor.DocumentName;
        if (name != "")
        {
            DocumentName = name;
        }
    }
    public async Task OnContentMenuSelect(CustomContentMenuEventArgs args)
    {
        if (args.Id.EndsWith("search_in_google"))
        {
            string selectedText = await container.DocumentEditor.Selection.GetTextAsync();
            string url = "http://google.com/search?q=" + selectedText;
            await JSRuntime.InvokeVoidAsync("open", new object[2] { url, "_blank" });
        }
    }
}


