@page "/speeddial/styles"
@using Syncfusion.Blazor.Buttons
@using Syncfusion.Blazor.Navigations
@using Syncfusion.Blazor.Popups
@*Hidden:Lines*@
@inherits SampleBaseComponent
@inject NavigationManager NavigationManager




@*End:Hidden*@
<SampleDescription>
    <p>
        This sample demonstrates how to customize the appearance of the Speed Dial action items. Click the speed dial button to open action items.
    </p>
</SampleDescription>
<ActionDescription>
    <p>
        In the above example, Speed Dial items appearance is customized using the <code>Text</code> and <code>IconCss</code> properties of <code>SpeedDialItem</code>.
    </p>
</ActionDescription>
<SfTooltip ID="Details" Target=".tooltip-speeddial .e-speeddial-li">
    <div class="speeddial-appearence-target custom-index" id="target"></div>
</SfTooltip>

<SfSpeedDial Content="Edit" Target="#target" Position="Syncfusion.Blazor.Buttons.FabPosition.BottomCenter" OpenIconCss="speeddial-icons speeddial-icon-edit" IconPosition="IconPosition.Left">
    <SpeedDialItems>
        <SpeedDialItem Text="Cut" IconCss="speeddial-icons speeddial-icon-cut" />
        <SpeedDialItem Text="Copy" IconCss="speeddial-icons speeddial-icon-copy" />
        <SpeedDialItem Text="Paste" IconCss="speeddial-icons speeddial-icon-paste" />
        <SpeedDialItem Text="Delete" IconCss="speeddial-icons speeddial-icon-delete" />
        <SpeedDialItem Text="Save" IconCss="speeddial-icons speeddial-icon-save" />
    </SpeedDialItems>
</SfSpeedDial>
<SfSpeedDial Content="Edit" Target="#target" Position="Syncfusion.Blazor.Buttons.FabPosition.BottomLeft">
    <SpeedDialItems>
        <SpeedDialItem Text="Cut" />
        <SpeedDialItem Text="Copy" />
        <SpeedDialItem Text="Paste" />
        <SpeedDialItem Text="Delete" />
        <SpeedDialItem Text="Save" />
    </SpeedDialItems>
</SfSpeedDial>
<SfSpeedDial Target="#target" CssClass="tooltip-speeddial" title="Edit" Position="Syncfusion.Blazor.Buttons.FabPosition.BottomRight" OpenIconCss="speeddial-icons speeddial-icon-edit">
    <SpeedDialItems>
        <SpeedDialItem Title="Cut" IconCss="speeddial-icons speeddial-icon-cut" />
        <SpeedDialItem Title="Copy" IconCss="speeddial-icons speeddial-icon-copy" />
        <SpeedDialItem Title="Paste" IconCss="speeddial-icons speeddial-icon-paste" />
        <SpeedDialItem Title="Delete" IconCss="speeddial-icons speeddial-icon-delete" />
        <SpeedDialItem Title="Save" IconCss="speeddial-icons speeddial-icon-save" />
    </SpeedDialItems>
</SfSpeedDial>

<style>
    .speeddial-appearence-target {
        min-height: 310px;
        width: 70%;
        margin: 5% 15%;
        position: relative;
    }

    @@media only screen and (max-width: 600px) {
        .speeddial-appearence-target {
            width: 90%;
            margin: 5%;
        }
    }

    .custom-index {
        z-index: 500 !important;
    }

    @@font-face {
        font-family: 'speeddial-icons';
        src: url(data:application/x-font-ttf;charset=utf-8;base64,) format('truetype');
        font-weight: normal;
        font-style: normal;
    }

    [class^="speeddial-icon-"], [class*=" speeddial-icon-"] {
        font-family: 'speeddial-icons' !important;
        speak: none;
        font-style: normal;
        font-weight: normal;
        font-variant: normal;
        text-transform: none;
        line-height: inherit;
        -webkit-font-smoothing: antialiased;
        -moz-osx-font-smoothing: grayscale;
    }

    .speeddial-icon-cut:before {
        content: "\e70a";
    }

    .speeddial-icon-copy:before {
        content: "\e70b";
    }

    .speeddial-icon-paste:before {
        content: "\e70c";
    }

    .speeddial-icon-delete:before {
        content: "\e70d";
    }

    .speeddial-icon-save:before {
        content: "\e70e";
    }

    .speeddial-icon-edit:before {
        content: "\e70f";
    }
</style>