@page "/speeddial/position"
@using Syncfusion.Blazor.Buttons
@using Syncfusion.Blazor.Navigations
@*Hidden:Lines*@
@inherits SampleBaseComponent
@inject NavigationManager NavigationManager




@*End:Hidden*@

<SampleDescription>
    <p>This sample demonstrates the different positions of the Speed Dial component in the target container. Based on the position of the speed dial, action items display direction will vary. Click the speed dial button to open action items.</p>
</SampleDescription>
<ActionDescription>
    <p>
        Speed dial displays action items based on <code>Position</code>, by default. Using the <code>Direction</code> property, specify one of the below directions.
        <ul>
            <li>Up</li>
            <li>Down</li>
            <li>Left</li>
            <li>Right</li>
        </ul>
    </p>
</ActionDescription>
<div class="control-section">
    <div id="target" class="speeddial-linear-target custom-index">
        <SfSpeedDial Target="#target" title="Top Left" Position="Syncfusion.Blazor.Buttons.FabPosition.TopLeft" CssClass="e-success" OpenIconCss="speeddial-icons speeddial-icon-upload">
            <SpeedDialItems>
                <SpeedDialItem IconCss="speeddial-icons speeddial-icon-image" Title="Image"/>
                <SpeedDialItem IconCss="speeddial-icons speeddial-icon-audio" Title="Audio"/>
                <SpeedDialItem IconCss="speeddial-icons speeddial-icon-video" Title="Video"/>
            </SpeedDialItems>
        </SfSpeedDial>
        <SfSpeedDial Target="#target" title="Top Center" Position="Syncfusion.Blazor.Buttons.FabPosition.TopCenter" CssClass="e-warning" OpenIconCss="speeddial-icons speeddial-icon-upload">
            <SpeedDialItems>
                <SpeedDialItem IconCss="speeddial-icons speeddial-icon-image" Title="Image"/>
                <SpeedDialItem IconCss="speeddial-icons speeddial-icon-audio" Title="Audio"/>
                <SpeedDialItem IconCss="speeddial-icons speeddial-icon-video" Title="Video"/>
            </SpeedDialItems>
        </SfSpeedDial>
        <SfSpeedDial Target="#target" title="Top Right" Position="Syncfusion.Blazor.Buttons.FabPosition.TopRight" CssClass="e-success" OpenIconCss="speeddial-icons speeddial-icon-upload">
            <SpeedDialItems>
