        <SfSpeedDial Target="#target" title="Middle Right" aria-label="Middle Right Speed Dial Menu" Position="Syncfusion.Blazor.Buttons.FabPosition.MiddleRight" CssClass="e-warning" Direction="LinearDirection.Left" OpenIconCss="speeddial-icons speeddial-icon-upload">
            <SpeedDialItems>
