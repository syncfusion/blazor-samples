                <SpeedDialItem IconCss="speeddial-icons speeddial-icon-audio" Title="Audio"/>
                <SpeedDialItem IconCss="speeddial-icons speeddial-icon-video" Title="Video"/>
            </SpeedDialItems>
        </SfSpeedDial>

        <SfSpeedDial Target="#target" title="Middle Left" Position="Syncfusion.Blazor.Buttons.FabPosition.MiddleLeft" CssClass="e-warning" Direction="LinearDirection.Right" OpenIconCss="speeddial-icons speeddial-icon-upload">
            <SpeedDialItems>
                <SpeedDialItem IconCss="speeddial-icons speeddial-icon-image" Title="Image"/>
                <SpeedDialItem IconCss="speeddial-icons speeddial-icon-audio" Title="Audio"/>
                <SpeedDialItem IconCss="speeddial-icons speeddial-icon-video" Title="Video"/>
            </SpeedDialItems>
        </SfSpeedDial>
        <SfSpeedDial Target="#target" title="Middle Center" Position="Syncfusion.Blazor.Buttons.FabPosition.MiddleCenter" OpenIconCss="speeddial-icons speeddial-icon-upload">
            <SpeedDialItems>
                <SpeedDialItem IconCss="speeddial-icons speeddial-icon-image" Title="Image"/>
                <SpeedDialItem IconCss="speeddial-icons speeddial-icon-audio" Title="Audio"/>
                <SpeedDialItem IconCss="speeddial-icons speeddial-icon-video" Title="Video"/>
            </SpeedDialItems>
        </SfSpeedDial>
        <SfSpeedDial Target="#target" title="Middle Right" Position="Syncfusion.Blazor.Buttons.FabPosition.MiddleRight" CssClass="e-warning" Direction="LinearDirection.Left" OpenIconCss="speeddial-icons speeddial-icon-upload">
            <SpeedDialItems>
                <SpeedDialItem IconCss="speeddial-icons speeddial-icon-image" Title="Image"/>
                <SpeedDialItem IconCss="speeddial-icons speeddial-icon-audio" Title="Audio"/>
                <SpeedDialItem IconCss="speeddial-icons speeddial-icon-video" Title="Video"/>
            </SpeedDialItems>
        </SfSpeedDial>
        <SfSpeedDial Target="#target" title="Bottom Left" Position="Syncfusion.Blazor.Buttons.FabPosition.BottomLeft" CssClass="e-success" OpenIconCss="speeddial-icons speeddial-icon-upload">
            <SpeedDialItems>
                <SpeedDialItem IconCss="speeddial-icons speeddial-icon-image" Title="Image"/>
                <SpeedDialItem IconCss="speeddial-icons speeddial-icon-audio" Title="Audio"/>
                <SpeedDialItem IconCss="speeddial-icons speeddial-icon-video" Title="Video"/>
            </SpeedDialItems>
        </SfSpeedDial>
        <SfSpeedDial Target="#target" title="Bottom Center" Position="Syncfusion.Blazor.Buttons.FabPosition.BottomCenter" CssClass="e-warning" OpenIconCss="speeddial-icons speeddial-icon-upload">
            <SpeedDialItems>
                <SpeedDialItem IconCss="speeddial-icons speeddial-icon-image" Title="Image"/>
                <SpeedDialItem IconCss="speeddial-icons speeddial-icon-audio" Title="Audio"/>
