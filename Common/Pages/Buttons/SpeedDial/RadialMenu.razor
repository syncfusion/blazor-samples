            <SpeedDialRadialSettings Direction="RadialDirection.Clockwise" OffSet="110px"/>
            <SpeedDialItems>
                <SpeedDialItem Title="Cut" IconCss="speeddial-icons speeddial-icon-cut" />
                <SpeedDialItem Title="Copy" IconCss="speeddial-icons speeddial-icon-copy" />
