@page "/fab/default-functionalities"
@using Syncfusion.Blazor.Buttons

@*Hidden:Lines*@
@inherits SampleBaseComponent
@inject NavigationManager NavigationManager




@*End:Hidden*@

<SampleDescription>
    <p>
        This sample demonstrates the default functionalities of the Floating Action Button (FAB).
    </p>
</SampleDescription>
<ActionDescription>
    <p>
        In this example, each FAB showcases the usage of <code>IconCss</code>, <code>CssClass</code>, <code>Content</code>, and <code>Disabled</code> properties, such as:
    </p>
    <ul>
        <li>Icon only FAB</li>
        <li>Icon with label FAB</li>
        <li>Disabled FAB</li>
        <li>Label Only FAB</li>
    </ul>
</ActionDescription>

<div class="control-section">
    <div id="fab-default-control">
        <div class="row">
            <div class="col-xs-6 col-sm-6 col-lg-6 col-md-6">
                <div id="target1" class="col-xs-12 col-sm-12 col-lg-6 col-md-6 fab-index">
                    <SfFab Target="#target1" Position="FabPosition.MiddleCenter" title="Mic" IconCss="fab-icons fab-icon-mic"></SfFab>
                </div>
                <div id="target2" class="col-xs-12 col-sm-12 col-lg-6 col-md-6 fab-index">
                    <SfFab Target="#target2" Position="FabPosition.MiddleCenter" Content="Contact" IconCss="fab-icons fab-icon-people"></SfFab>
                </div>
            </div>
            <div class="col-xs-6 col-sm-6 col-lg-6 col-md-6">
                <div id="target3" class="col-xs-12 col-sm-12 col-lg-6 col-md-6 fab-index">
                    <SfFab Target="#target3" Position="FabPosition.MiddleCenter" Content="Disabled" Disabled="true" IconCss="fab-icons fab-icon-people"></SfFab>
                </div>
                <div id="target4" class="col-xs-12 col-sm-12 col-lg-6 col-md-6 fab-index">
                    <SfFab Target="#target4" Position="FabPosition.MiddleCenter" Content="Text Content" CssClass="e-warning"></SfFab>
                </div>
            </div>
        </div>
    </div>
</div>



<style>
    #fab-default-control {
        width: 90%;
        margin: 8% auto;
        text-align: center;
        max-width: 600px;
    }

    .fab-index {
        z-index: 500 !important;
    }

    #fab-default-control .col-xs-12 {
        position: relative;
        margin: 15px 0;
        min-height: 100px;
    }

    #fab-default-control .col-md-6 {
        padding: 0;
    }
    @@media only screen and (max-width: 600px) {
        .fab-appearence-container .col-xs-12 {
            min-height: 110px;
        }
    }

    @@font-face {
        font-family: 'fab-icons';
        src: url(data:application/x-font-ttf;charset=utf-8;base64,) format('truetype');
        font-weight: normal;
        font-style: normal;
    }

    [class^="fab-icon-"],
    [class*=" fab-icon-"] {
        font-family: 'fab-icons' !important;
        speak: none;
        font-size: 55px;
        font-style: normal;
        font-weight: normal;
        font-variant: normal;
        text-transform: none;
        line-height: inherit;
        -webkit-font-smoothing: antialiased;
        -moz-osx-font-smoothing: grayscale;
    }

    .fab-icon-mic:before {
        content: "\e708";
    }

    .fab-icon-people:before {
        content: "\e70a";
    }
</style>
