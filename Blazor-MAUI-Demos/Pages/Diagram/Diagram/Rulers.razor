        CreateNode("Board", 300, 380, NodeFlowShapes.Decision, "Board decides \n whether to proceed", 150, 110);
        CreateNode("Find", 300, 530, NodeFlowShapes.Decision, "Find project \n manager");
        CreateNode("Implement", 300, 655, NodeFlowShapes.Process, "Implement and deliver");
        CreateNode("Decision", 530, 160, NodeFlowShapes.Card, "Decision process for new software ideas", 250, 60);
        CreateNode("Reject", 550, 380, NodeFlowShapes.Process, "Reject");
        CreateNode("Hire", 550, 530, NodeFlowShapes.Process, "Hire new resources");
